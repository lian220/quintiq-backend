# 한국투자증권 API 설정
KIS_BASE_URL=https://openapivts.koreainvestment.com:29443
KIS_REAL_URL=https://openapi.koreainvestment.com:9443
KIS_APPKEY=your_kis_appkey_here
KIS_APPSECRET=your_kis_appsecret_here
KIS_CANO=your_account_number
KIS_ACNT_PRDT_CD=01
KIS_USE_MOCK=true
TR_ID=VTTC8434R

# FRED API Key 설정
FRED_API_KEY=your_fred_api_key_here

# Supabase 설정
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# MongoDB 설정
MONGO_URL=mongodb+srv://your-cluster.mongodb.net
MONGO_USER=your_mongo_user
MONGO_PASSWORD=your_mongo_password
USE_MONGODB=true

# AlphaVantage API 키
ALPHA_VANTAGE_API_KEY=your_alphavantage_api_key_here

# 기타 환경 설정
DEBUG=true

# 한국투자증권 API 설정
# 실제투자 사용 (모의투자: true, 실제투자: false)
KIS_USE_MOCK=false

# ========================================
# Slack 알림 설정
# ========================================

# Slack 알림 설정 (선택사항)
SLACK_WEBHOOK_URL_TRADING=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
SLACK_WEBHOOK_URL_ANALYSIS=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
SLACK_WEBHOOK_URL_SCHEDULER=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
SLACK_ENABLED=true

# Google Cloud 설정
GCP_PROJECT_ID=your_gcp_project_id
GCP_REGION=us-central1
GCP_BUCKET_NAME=your_bucket_name

# Vertex AI 방법 선택
USE_TRAINING_JOBS=true  # true: Training Jobs, false: Custom Jobs

# Vertex AI 설정
VERTEX_AI_MACHINE_TYPE=n1-standard-4
VERTEX_AI_GPU_TYPE=NVIDIA_TESLA_T4
VERTEX_AI_GPU_COUNT=1
VERTEX_AI_CONTAINER_URI=

GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/credentials/vertex-ai-key.json

RUN_ECONOMIC_DATA_ON_STARTUP=false
VERTEX_AI_USE_FLEX_START=false
GITHUB_TOKEN=your_github_token_here
